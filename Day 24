public static Node removeDuplicates(Node head){
        if (head == null) return null;

        Node current = head;
        while (current.next != null)
        {
            if (current.data == current.next.data)
            {
                // Skip duplicate
                current.next = current.next.next;
            }
            else
            {
                current = current.next;
            }
        }
        return head;
    }
